{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "module": "commonjs",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "paths": {
            "@plone/volto/*": [
                "../node_modules/@plone/volto/src/*",
                "../node_modules/@plone/volto/types/*"
            ],
            "@root/*": [
                "."
            ],
            "browser": [
                "./browser"
            ],
            "querystring": [
                "./browser/querystring"
            ],
            "volto-camara-de-curitiba": [
                "addons/volto-camara-de-curitiba/src"
            ],
            "volto-slider-banner": [
                "addons/volto-slider-banner/src"
            ],
            "volto-columns-block": [
                "addons/volto-columns-block/src"
            ],
            "volto-mui-blocks": [
                "addons/volto-mui-blocks/src"
            ],
            "volto-mui-banners": [
                "addons/volto-mui-banners/src"
            ],
            "partidos-addon": [
                "addons/partidos-addon/src"
            ]
        },
        "baseUrl": "src"
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "build",
        "public",
        "coverage",
        "src/**/*.test.{js,jsx,ts,tsx}",
        "src/**/*.spec.{js,jsx,ts,tsx}",
        "src/**/*.stories.{js,jsx,ts,tsx}"
    ]
}